/* pages/search/search.wxss */
.left{
  float:left;
}
.search-hot{
  height:100rpx;
  padding:32rpx 32rpx;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
}
.search-ad{
  width:100%;
}
.search-ad image{
  width:100%;
  height:200rpx;
}
.search-hot-word{
  width:100%;
  padding:0 32rpx;
}
.search-hot-word view{
  float:left;
  background-color:rgb(250, 232, 232);
  border:1px solid rgb(227, 175, 150);
  margin-right:16rpx;
  margin-bottom:16rpx;
  padding:16rpx 16rpx;
}